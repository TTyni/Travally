// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}



model x_world {
  fieldid Int @id
  tid Int
  vid Int
  villagename String
  playerid Int
  playername String
  allianceid Int
  alliance String
  population Int
  region String
  capital Boolean
  city Boolean
  harbor Boolean
  victorypoints Int
  tags tags[]
}

model users {
  username String 
  playerid Int @id
  password String
}

model tags {   
  id Int @id @default(autoincrement())
  fielddata x_world @relation(fields: [id], references: [fieldid], onDelete: Cascade)
  off Boolean
  def Boolean
  target Boolean
  }
  
model incomings {
  fielddata Int @id
  x Int
  y Int
  arrivaltime DateTime
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}